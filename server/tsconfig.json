{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "pretty": true,
    "baseUrl": "./",
    "types": [
      "node"
    ],
    "typeRoots": [
      "./node_modules/@types/"
    ],
    "paths": {
      "@@/*": [
        "src/*",
        "api/Binance"
      ],
      "@/*": [
        "config/config.json"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}